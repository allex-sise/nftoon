  "source": "// SPDX-License-Identifier: MIT OR Apache-2.0\r\n\r\npragma solidity ^0.8.0;\r\n\r\nimport \"@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol\";\r\nimport \"@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol\";\r\n\r\n/**\r\n * @notice A mixin that stores a reference to the Minted treasury contract.\r\n */\r\nabstract contract MintedTreasuryNode is Initializable {\r\n  using AddressUpgradeable for address payable;\r\n\r\n  address payable private treasury;\r\n\r\n  /**\r\n   * @dev Called once after the initial deployment to set the Minted treasury address.\r\n   */\r\n  function _initializeMintedTreasuryNode(address payable _treasury) internal initializer {\r\n    require(_treasury.isContract(), \"MintedTreasuryNode: Address is not a contract\");\r\n    treasury = _treasury;\r\n  }\r\n\r\n  /**\r\n   * @notice Returns the address of the Minted treasury.\r\n   */\r\n  function getMintedTreasury() public view returns (address payable) {\r\n    return treasury;\r\n  }\r\n\r\n  // `______gap` is added to each mixin to allow adding new data slots or additional mixins in an upgrade-safe way.\r\n  uint256[2000] private __gap;\r\n}",
